{
    "name": "mrwadson/php-cacher",
    "description": "Just a simple PHP Cache class",
    "keywords": [
        "php", "cache", "cacher", "php-cache", "php-cacher", "storage", "file-storage"
    ],
    "homepage": "https://github.com/mrwadson",
    "authors": [
        {
            "name": "Vadim Danilov",
            "email": "mrwadson@mail.ru"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "mrwadson\\cacher\\": "src/"
        }
    },
    "scripts": {
